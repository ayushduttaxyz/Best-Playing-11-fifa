        "# goal keeping characteristic\n",
        "df['gk_Shot_Stopper'] = (b*df.Reactions + b*df.Composure + a*df.SprintSpeed + a*df.Strength + c*df.Jumping + b*df.GKPositioning + c*df.GKReflexes + b*df.GKHandling)/(2*a + 4*b + 2*c )\n",
        "\n",
        "df['gk_Skeeper'] = (b*df.Reactions + b*df.Composure + b*df.SprintSpeed + a*df.ShortPassing + a*df.LongPassing + b*df.Jumping + b*df.GKPositioning + b*df.GKDiving + d*df.GKReflexes + b*df.GKHandling + d*df.GKKicking + c*df.Vision)/(2*a + 4*b + 1*c + 1*d)"
