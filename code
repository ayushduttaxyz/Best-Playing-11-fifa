        "df['mf_playmaker'] = (d*df.BallControl + d*df.Dribbling + a*df.Marking + d*df.Reactions + d*df.Vision + c*df.Positioning + c*df.Crossing + d*df.ShortPassing + c*df.LongPassing + c*df.Curve + b*df.LongShots + c*df.FKAccuracy)/(1*a + 1*b + 4*c + 4*d)\n",
        "df['mf_beast'] = (d*df.Agility + c*df.Balance + b*df.Jumping + c*df.Strength + d*df.Stamina + a*df.SprintSpeed + c*df.Acceleration + d*df.ShortPassing + c*df.Aggression + d*df.Reactions + b*df.Marking + b*df.StandingTackle + b*df.SlidingTackle + b*df.Interceptions)/(1*a + 5*b + 4*c + 4*d)\n",
        "df['mf_controller'] = (d*df.BallControl + a*df.Dribbling + a*df.Marking + a*df.Reactions + c*df.Vision + c*df.Composure + d*df.ShortPassing + d*df.LongPassing)/(2*c + 3*d + 4*a)"
